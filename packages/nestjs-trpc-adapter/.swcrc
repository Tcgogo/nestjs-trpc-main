{
  "$schema": "https://swc.rs/schema.json#",
  "sourceMaps": true,
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "baseUrl": "./",
    "target": "es5"
  },
  "module": {
    "type": "es6",
    "resolveFully": true,
    "outFileExtension": "js"
  },

  "minify": false
}
